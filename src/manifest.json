      "handlers": {
        "run": "MapColorsAndCreateVariables",
        "actions": {
          "Shutdown": "onShutdown"
        }
      }
    }
  ],
  "menu": {
    "title": "Map color variables",
    "items": [
      "mapColorVariables",
      "mapColorVariablesAndCreateNewOnes"
    ]
  }
